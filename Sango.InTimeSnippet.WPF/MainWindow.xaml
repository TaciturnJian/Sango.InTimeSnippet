<Window x:Class="Sango.InTimeSnippet.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sango.InTimeSnippet.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        WindowStartupLocation="CenterScreen" Topmost="True" WindowStyle="None" MouseDown="Window_MouseDown"
        >
    <Grid MouseDown="Grid_MouseDown">
        <local:SnippetBoxes x:Name="SnippetBox" Margin="4,4,4,4"/>
    </Grid>
</Window>
